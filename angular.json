{
  "version": 1,
  "cli": {
    "defaultCollection": "@nrwl/angular"
  },
  "defaultProject": "music-store",
  "schematics": {
    "@nrwl/workspace": {
      "library": {
        "linter": "tslint"
      }
    },
    "@nrwl/cypress": {
      "cypress-project": {
        "linter": "tslint"
      }
    },
    "@nrwl/node": {
      "application": {
        "linter": "tslint"
      },
      "library": {
        "linter": "tslint"
      }
    },
    "@nrwl/nest": {
      "application": {
        "linter": "tslint"
      },
      "library": {
        "linter": "tslint"
      }
    },
    "@nrwl/express": {
      "application": {
        "linter": "tslint"
      },
      "library": {
        "linter": "tslint"
      }
    },
    "@nrwl/angular:application": {
      "style": "scss",
      "linter": "tslint",
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "style": "scss",
      "linter": "tslint",
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:component": {
      "style": "scss"
    }
  },
  "projects": {
    "music-store": {
      "projectType": "application",
      "root": "apps/music-store",
      "sourceRoot": "apps/music-store/src",
      "prefix": "music-store",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/music-store",
            "index": "apps/music-store/src/index.html",
            "main": "apps/music-store/src/main.ts",
            "polyfills": "apps/music-store/src/polyfills.ts",
            "tsConfig": "apps/music-store/tsconfig.app.json",
            "aot": true,
            "assets": [
              "apps/music-store/src/favicon.ico",
              "apps/music-store/src/assets"
            ],
            "styles": ["./node_modules/@angular/material/prebuilt-themes/indigo-pink.css", "apps/music-store/src/styles.scss"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/music-store/src/environments/environment.ts",
                  "with": "apps/music-store/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "music-store:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "music-store:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "music-store:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/music-store/tsconfig.app.json",
              "apps/music-store/tsconfig.spec.json",
              "apps/music-store/tsconfig.editor.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/music-store/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/music-store"],
          "options": {
            "jestConfig": "apps/music-store/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "music-store-e2e": {
      "root": "apps/music-store-e2e",
      "sourceRoot": "apps/music-store-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/music-store-e2e/cypress.json",
            "tsConfig": "apps/music-store-e2e/tsconfig.e2e.json",
            "devServerTarget": "music-store:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "music-store:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": ["apps/music-store-e2e/tsconfig.e2e.json"],
            "exclude": ["**/node_modules/**", "!apps/music-store-e2e/**/*"]
          }
        }
      }
    },
    "tracks-list-shell": {
      "projectType": "library",
      "root": "libs/tracks-list/shell",
      "sourceRoot": "libs/tracks-list/shell/src",
      "prefix": "ms",
      "architect": {
        "build": {
          "builder": "@nrwl/angular:ng-packagr-lite",
          "options": {
            "tsConfig": "libs/tracks-list/shell/tsconfig.lib.json",
            "project": "libs/tracks-list/shell/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "libs/tracks-list/shell/tsconfig.lib.prod.json"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/tracks-list/shell/tsconfig.lib.json",
              "libs/tracks-list/shell/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!libs/tracks-list/shell/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/tracks-list/shell"],
          "options": {
            "jestConfig": "libs/tracks-list/shell/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "tracks-list-ui": {
      "projectType": "library",
      "root": "libs/tracks-list/ui",
      "sourceRoot": "libs/tracks-list/ui/src",
      "prefix": "ms",
      "architect": {
        "build": {
          "builder": "@nrwl/angular:ng-packagr-lite",
          "options": {
            "tsConfig": "libs/tracks-list/ui/tsconfig.lib.json",
            "project": "libs/tracks-list/ui/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "libs/tracks-list/ui/tsconfig.lib.prod.json"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/tracks-list/ui/tsconfig.lib.json",
              "libs/tracks-list/ui/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!libs/tracks-list/ui/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/tracks-list/ui"],
          "options": {
            "jestConfig": "libs/tracks-list/ui/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "tracks-list-feature": {
      "projectType": "library",
      "root": "libs/tracks-list/feature",
      "sourceRoot": "libs/tracks-list/feature/src",
      "prefix": "ms",
      "architect": {
        "build": {
          "builder": "@nrwl/angular:ng-packagr-lite",
          "options": {
            "tsConfig": "libs/tracks-list/feature/tsconfig.lib.json",
            "project": "libs/tracks-list/feature/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "libs/tracks-list/feature/tsconfig.lib.prod.json"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/tracks-list/feature/tsconfig.lib.json",
              "libs/tracks-list/feature/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!libs/tracks-list/feature/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/tracks-list/feature"],
          "options": {
            "jestConfig": "libs/tracks-list/feature/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "tracks-list-util": {
      "projectType": "library",
      "root": "libs/tracks-list/util",
      "sourceRoot": "libs/tracks-list/util/src",
      "prefix": "ms",
      "architect": {
        "build": {
          "builder": "@nrwl/angular:ng-packagr-lite",
          "options": {
            "tsConfig": "libs/tracks-list/util/tsconfig.lib.json",
            "project": "libs/tracks-list/util/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "libs/tracks-list/util/tsconfig.lib.prod.json"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/tracks-list/util/tsconfig.lib.json",
              "libs/tracks-list/util/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!libs/tracks-list/util/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/tracks-list/util"],
          "options": {
            "jestConfig": "libs/tracks-list/util/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "tracks-list-data-access": {
      "projectType": "library",
      "root": "libs/tracks-list/data-access",
      "sourceRoot": "libs/tracks-list/data-access/src",
      "prefix": "ms",
      "architect": {
        "build": {
          "builder": "@nrwl/angular:ng-packagr-lite",
          "options": {
            "tsConfig": "libs/tracks-list/data-access/tsconfig.lib.json",
            "project": "libs/tracks-list/data-access/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "libs/tracks-list/data-access/tsconfig.lib.prod.json"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/tracks-list/data-access/tsconfig.lib.json",
              "libs/tracks-list/data-access/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**",
              "!libs/tracks-list/data-access/**/*"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/tracks-list/data-access"],
          "options": {
            "jestConfig": "libs/tracks-list/data-access/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  }
}
